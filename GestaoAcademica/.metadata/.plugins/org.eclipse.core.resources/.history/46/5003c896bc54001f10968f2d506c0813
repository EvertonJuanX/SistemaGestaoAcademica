package br.ifpe.edu.akadmik.gestao.Projeto.endereco;

import jakarta.validation.constraints.NotBlank;

public record DadosCadastroEndereco(

		@NotBlank
		Long id,

		@NotBlank
		String estado, 
		
		@NotBlank
		String cidade,
		
		@NotBlank
		String cep, 
		
		@NotBlank
		String bairro, 
		
		@NotBlank
		String rua, 
		
		@NotBlank
		String numero, 
		
		@NotBlank
		String complemento

) {

}
